<h1>User</h1>

<ul>
<% @users.each do |user| %>
  <li>
    <%= link_to user.name, user %>
    <% if @current_user.nil? %>
      <%= form_tag sessions_path do %>
        <%= hidden_field_tag :user_id, user.id %>
        <%= submit_tag "Sign in as user" %>
      <% end %>
    <% end %>
  </li>
<% end %>
</ul>
